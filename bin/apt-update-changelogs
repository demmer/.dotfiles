#!/usr/bin/python

import sys, re
from urllib2 import *;

urlpat = "http://packages.debian.org/changelogs/pool/main/%s/%s/%s_%s/changelog";

input = sys.stdin.readlines();

packages = []

for l in input:
    p = re.compile("^Inst ((\S)\S+) \[(.*)\] \((\S+)");
    m = p.match(l);
    if (m == None):
        continue

    pkg = m.group(1);
    ltr = m.group(2);
    old = m.group(3);
    new = m.group(4);
    packages.append((pkg, ltr, old, new));

packages.sort()

for (pkg, ltr, old, new) in packages:
    print "%s (%s -> %s)" % (pkg, old, new) ,
    
    url = urlpat % (ltr, pkg, pkg, new)

    try:
        log = urlopen(url);
    except HTTPError, e:
        print "\n(no changelog)\n"
        continue

    pkgpat = re.compile("^%s \((.*)\) (.*)" % pkg);
    whitepat = re.compile("^\s+$");
    
    for l in log:
        if (whitepat.match(l)):
            continue

        m = pkgpat.match(l);
        if (m):
            if (m.group(1) != new):
                break
            else:
                print m.group(2);
                continue

        print l ,

    print "\n"
