[alias]
	st = status
	ci = commit
	cp = cherry-pick
	rbm = rebase origin/master
[user]
	name = Michael Demmer
	email = mdemmer@slack-corp.com
[core]
	pager = less -F -X -r
	excludesfile = /Users/demmer/.gitignore_global
	editor = /usr/bin/vim

[color]
	ui = true
[color "status"]
	untracked = white
[push]
	default = current
[fetch]
	prune = true
