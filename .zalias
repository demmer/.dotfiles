#  architecture dependant
if [ "$ARCH" = "solaris" ]; then
	alias mine="ps -fu $LOGNAME"
	alias other="ps -ef | egrep -v '^ *root|^ *$LOGNAME|^ *www|^ *daemon'"
elif [ "$ARCH" = "Linux" ]; then
	alias mine="ps -aux | fgrep $LOGNAME"
	alias other="ps -aux | egrep -v '^ *root|^ *$LOGNAME|^ *www|^ *daemon'"
elif [ "$ARCH" = "sunos" ]; then
	alias mine="ps -aux | fgrep $LOGNAME"
	alias other="ps -aux | egrep -v '^ *root|^ *$LOGNAME|^ *www|^ *daemon'"
else
	alias psu='echo "Unknown architecture."'
	alias notme='echo "Unknown architecture."'
fi
