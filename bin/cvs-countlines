#!/usr/bin/perl

%peeps;
open (CVS, "/bin/sh -c 'cvs annotate @ARGV 2> /dev/null'|") || die "can't run cvs";
while(<CVS>) {
    @L = split;
    $who = $L[1];
    $who =~ s/\(//;
    $peeps{$who}++;
}

foreach $who (sort { $peeps{$b} <=> $peeps{$a} } keys %peeps) {
    printf("%10s: %s\n", $who, $peeps{$who});
}
